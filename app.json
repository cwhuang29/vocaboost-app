{
  "expo": {
    "name": "vocabulary-highlighter-app",
    "slug": "vocabulary-highlighter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "vocabulary_highlighter_app_bundle_id",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "vocabulary_highlighter_app_bundle_id",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
